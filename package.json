{
  "name": "webpack-vue",
  "version": "1.0.0",
  "description": "Vue project template using webpack",
  "main": "src/index.ts",
  "scripts": {
    "build": "yarn prepare && webpack --env production --config webpack.config.js",
    "build:dev": "yarn prepare && webpack --env development --config webpack.config.js",
    "lint": "eslint --fix",
    "prepare": "rm -rf ./dist",
    "start:build:local": "node scripts/start/buildLocal.js",
    "start:build:prod": "docker compose -f docker-compose.yml up --build",
    "start:dev": "node scripts/start/dev.js",
    "start": "docker compose -f docker-compose.dev.yml up --build",
    "stop": "docker compose down",
    "stop:build:local": "node scripts/stop/buildLocal.js",
    "stop:build:prod": "docker compose down",
    "test": "jest"
  },
  "keywords": [
    "vue",
    "template",
    "webpack"
  ],
  "author": "bonaxl015",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/eslint-parser": "^7.26.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-flow": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@mdi/js": "^7.4.47",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-webpack-plugin": "^4.2.0",
    "file-loader": "^6.2.0",
    "globals": "^16.0.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.1.3",
    "prettier": "^3.5.1",
    "sass": "^1.85.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.7.3",
    "vue-eslint-parser": "^9.4.3",
    "vue-loader": "^17.4.2",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.97.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "5.2.1",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify": "^3.7.13"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      "ie > 6",
      ">1%",
      "not dead",
      "not op_mini all"
    ]
  },
  "engines": {
    "npm": ">=10",
    "node": ">=20"
  }
}
